
[push]
    default = matching
[gitflow "prefix"]
    feature = feature-
    release = release-
    hotfix = hotfix-
    support = support-
    versiontag = v
[alias]
    br = branch
    s = status
    c = commit
    l = log --oneline
    cleanup = clean -fdx
    co = checkout
[color]
    branch = auto
    diff = auto
    grep = auto
    interactive = auto
    showbranch = auto
    status = auto
    ui = auto
[core]
	editor = code --wait
	autocrlf = false
[pager]
	branch = false
