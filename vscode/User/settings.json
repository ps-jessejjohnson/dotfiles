{
    "update.mode": "start",
    "files.autoSave": "onFocusChange",
    "workbench.colorTheme": "LaserWave",
    "window.zoomLevel": 0,
    "workbench.startupEditor": "none",
    "explorer.openEditors.visible": 0,
    "workbench.tree.indent": 20,
    "workbench.iconTheme": "vscode-icons",
    "editor.codeLens": true,
    "editor.hover.delay": 40,
    "editor.wordWrapColumn": 120,
    "editor.fontFamily": "Dank Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.quickSuggestionsDelay": 5,
    "azureFunctions.projectLanguage": "C#",
    "workbench.tree.renderIndentGuides": "always",
    "workbench.activityBar.visible": true,
    "javascript.preferences.quoteStyle": "single",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontFamily": "Dank Mono",
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.rightClickBehavior": "paste",
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "terraform.codelens.enabled": false,
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "zenMode.hideLineNumbers": false,
    "window.title": "${rootName} ðŸ”¥ ${activeEditorShort}${dirty}",
    "workbench.settings.useSplitJSON": true,
    "workbench.settings.editor": "json",
    "git.autofetch": true,
    "terraform.telemetry.enabled": false,
    "vsicons.dontShowNewVersionMessage": true,
    "markdown.extension.tableFormatter.enabled": false,
    "explorer.compactFolders": false,
    "workbench.commandPalette.history": 10,
    "workbench.editor.highlightModifiedTabs": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.fontAliasing": "default",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "vsonline.accountProvider": "Microsoft",
    "githubPullRequests.showInSCM": false,
    "dotfiles.repository": "https://github.com/ThorstenHans/dotfiles.git",
}